Current Chats:
<ul>
 <% if @existing_chats_users %>
   <% @existing_chats_users.each do |user| %>
    <% if user != current_user %>
      <li>
        <%= link_to(user_chats_path(current_user, :user => user.id), method: :post) do %>
        <%= user.username %>
        <% end %>
      </li>
     <% end %>
   <% end %>
  <% end %>
</ul>
All Users:
<ul>
  <% User.all.each do |user| %>
   <% if user != current_user %>
     <li>
       <%= link_to(user_chats_path(current_user, :user => user.id), method: :post) do %>
       <%= user.username %>
       <% end %>
     </li>
    <% end %>
  <% end %>
</ul>
